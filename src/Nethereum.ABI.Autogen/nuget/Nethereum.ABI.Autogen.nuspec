<?xml version="1.0"?>
<package >
  <metadata>
    <id>Nethereum.ABI.Autogen</id>
    <version>1.0.0.1</version>
    <authors>Dave Whiffin, Juan Blanco</authors>
    <owners>Dave Whiffin, Juan Blanco</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>Nethereum: Solidity to DotNet code generator</summary>
    <description>Generates dot net code for Ethereum (solidity) contracts.  Just add a Nethereum.Generator.config file to the root of the project.</description>
    <dependencies>
      <group targetFramework=".NETCoreApp2.0">
        <dependency id="Nethereum.Web3" version="2.4.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="..\bin\debug\netcoreapp2.0\publish\*.dll" target="tools" />
    <file src="..\bin\debug\netcoreapp2.0\publish\*.json" target="tools" />
    <file src="Nethereum.ABI.Autogen.targets" target="build" />
  </files>

</package>